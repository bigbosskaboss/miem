import telebot as tb
from telebot import types
bot = tb.TeleBot("1243726622:AAGrVuKUackFljYRgLG_8Ptj2zEpzBByc5A")


def get_start_keyboard():
    start_keyboard = types.InlineKeyboardMarkup()
    button_join = types.InlineKeyboardButton(text="–°–ø–∏—Å–æ–∫ –¥–æ—Å—Ç—É–ø–Ω—ã—Ö –Ω–æ—á–Ω—ã—Ö —Ç–æ—á–µ–∫", callback_data='join')
    start_keyboard.add(button_join)
    return start_keyboard


@bot.message_handler(commands=['start'])
def get_messages(message):
    if message.text == '/start':
        bot.send_message(message.from_user.id, "–ü—Ä–∏–≤–µ—Ç—Å—Ç–≤—É—é —Ç–µ–±—è, –ê–¥–µ–ø—Ç!\n"    
"–Ø - —Ç–≤–æ–π –ø—Ä–æ–≤–æ–¥–Ω–∏–∫ –≤ –Ω–æ—á–Ω–æ–µ –≤–µ—Å–µ–ª—å–µ.\n"
"–ü—Ä–∏ –ø–æ–º–æ—â–∏ –¥–∞–Ω–Ω–æ–≥–æ –±–æ—Ç–∞ —Ç—ã —Å–º–æ–∂–µ—à—å —É–∑–Ω–∞—Ç—å –æ –≤—Å–µ—Ö –¥–æ—Å—Ç—É–ø–Ω—ã—Ö –Ω–æ—á–Ω—ã—Ö —Ç–æ—á–∫–∞—Ö, –∏—Ö —Ä–∞—Å–ø–æ–ª–æ–∂–µ–Ω–∏–∏ –∏ –≥—Ä–∞—Ñ–∏–∫–µ —Ä–∞–±–æ—Ç—ã.", reply_markup=get_start_keyboard())

@bot.callback_query_handler(func=lambda call: True)
def callback_worker(call):
    if call.data == 'join':
        join_keyboard = types.InlineKeyboardMarkup()
        button_tochka_1 = types.InlineKeyboardButton(text='–ê–ª–∫–æ–º–∞—Ä–∞—Ñ–æ–Ωüî•', callback_data='1')
