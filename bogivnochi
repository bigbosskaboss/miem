        button_tochka_8 = types.InlineKeyboardButton(text='–ú—É–∑. –®–æ—Ç—ãüî•', callback_data='8')
        button_tochka_9 = types.InlineKeyboardButton(text='–ß—Ç–æ –í–∞–º –ù—Ä–∞–≤–∏—Ç—Å—èüî•', callback_data='9')
        button_tochka_10 = types.InlineKeyboardButton(text='–ê–Ω–≥–µ–ª –∏ –î–µ–º–æ–Ω', callback_data='10')
        button_tochka_11 = types.InlineKeyboardButton(text='–û–ª–∏–º–ø 2', callback_data='11')
        button_tochka_12 = types.InlineKeyboardButton(text='–£–º–Ω–∏—Ü—ã –∏ –ü—å—è–Ω–∏—Ü—ã', callback_data='12')
        button_tochka_13 = types.InlineKeyboardButton(text='–û—Ä–µ–ª –∏–ª–∏ –†–µ—à–∫–∞üî•', callback_data='13')
        join_keyboard.add(button_tochka_1)
        join_keyboard.add(button_tochka_2)
        join_keyboard.add(button_tochka_3)
        join_keyboard.add(button_tochka_4)
        join_keyboard.add(button_tochka_5)
        join_keyboard.add(button_tochka_6)
        join_keyboard.add(button_tochka_7)
        join_keyboard.add(button_tochka_8)
        join_keyboard.add(button_tochka_9)
        join_keyboard.add(button_tochka_10)
        join_keyboard.add(button_tochka_11)
        join_keyboard.add(button_tochka_12)
        join_keyboard.add(button_tochka_13)

        bot.edit_message_text(chat_id=call.message.chat.id, message_id=call.message.message_id,
                              text="–≤—ã–±–µ—Ä–∏ —Ç–æ—á–∫—É", reply_markup=join_keyboard)
    if call.data == '1':
        keyboard = types.InlineKeyboardMarkup()
        button_cancel = types.InlineKeyboardButton(text="–ù–∞–∑–∞–¥", callback_data='back')
